<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ppl.b08.warunglaundry">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.pengguna.HomeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.pengguna.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.pengguna.RegisterActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".view.penyedia.HomeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".view.penyedia.LoginActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".view.penyedia.RegisterActivity"/>
    </application>

</manifest>
